{
  "name": "extract-plugin",
  "version": "0.2.1",
  "description": "Extract any files from webpack build.",
  "license": "MIT",
  "author": "Matthias Giger",
  "contributors": [
    {
      "name": "Tobias Koppers @sokra (mini-css-extract-plugin)"
    }
  ],
  "repository": "github:tobua/extract-plugin",
  "main": "dist/cjs.js",
  "scripts": {
    "start": "npm run build -- -w",
    "build": "cross-env NODE_ENV=production babel src -d dist --ignore 'src/**/*.test.js' --copy-files",
    "clean": "del-cli dist",
    "lint": "eslint --cache --fix src test",
    "lint-staged": "lint-staged",
    "prebuild": "npm run clean",
    "prepublish": "npm run build",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:update": "jest -u",
    "test:manual": "webpack-dev-server test/manual/src/index.js --open --config test/manual/webpack.config.js",
    "defaults": "webpack-defaults"
  },
  "files": [
    "dist"
  ],
  "peerDependencies": {
    "webpack": "^4.43.0"
  },
  "dependencies": {
    "@webpack-contrib/schema-utils": "^1.0.0-beta.0",
    "loader-utils": "^2.0.0",
    "webpack-sources": "^1.4.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.5",
    "@babel/core": "^7.10.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.10.4",
    "@babel/polyfill": "^7.10.4",
    "@babel/preset-env": "^7.10.4",
    "@webpack-contrib/eslint-config-webpack": "^3.0.0",
    "babel-jest": "^26.1.0",
    "cross-env": "^7.0.2",
    "css-loader": "^3.6.0",
    "del": "^5.1.0",
    "del-cli": "^3.0.1",
    "eslint": "^7.5.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-prettier": "^3.1.4",
    "file-loader": "^6.0.0",
    "globby": "^11.0.1",
    "jest": "^26.1.0",
    "lint-staged": "^10.2.11",
    "lodash.get": "^4.4.2",
    "memory-fs": "^0.5.0",
    "mini-css-extract-plugin": "^0.9.0",
    "prettier": "^2.0.5",
    "raw-loader": "^4.0.1",
    "rimraf": "^3.0.2",
    "webpack": "^4.17.2",
    "webpack-cli": "^3.3.12",
    "webpack-defaults": "^4.0.0",
    "webpack-dev-server": "^3.11.0"
  },
  "keywords": [
    "webpack",
    "extract",
    "plugin"
  ],
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  }
}
